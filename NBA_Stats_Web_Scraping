    {
      "cell_type": "code",
      "source": [
        "df = pd.DataFrame(columns=df_cols) #empty DataFrame to store scraped data\n",
        "season_types = ['Regular Season', 'Playoffs'] #types of seasons to scrape data for\n",
        "years =  ['2015-16', '2016-17', '2017-18', '2018-19', '2019-20', '2020-21', '2021-22', '2022-23', '2023-24'] # Define the range of years to scrape data for\n",
        "\n",
        "begin_loop = time.time() #start time\n",
        "for y in years: #loop through each year\n",
        "    for s in season_types: #loop through each season type (Regular Season, Playoffs)\n",
        "      api_url = \"https://stats.nba.com/stats/leagueLeaders?LeagueID=00&PerMode=Totals&Scope=S&Season=\"+y+\"&SeasonType=\"+s+\"&StatCategory=PTS\" # Construct the API URL for the current year and season type, requesting points leaders\n",
        "      try: #error handler\n",
        "          r = requests.get(url=api_url, headers=headers).json()\n",
        "          if 'resultSets' in r and r['resultSets'] and 'rowSet' in r['resultSets'][0] and r['resultSets'][0]['rowSet']:\n",
        "              temp_df1 = pd.DataFrame(r['resultSets'][0]['rowSet'], columns=table_headers)\n",
        "              temp_df2 = pd.DataFrame({'Year':[y for i in range(len(temp_df1))],\n",
        "                                      'Season_type': [s for i in range(len(temp_df1))]})\n",
        "\n",
        "              temp_df3 = pd.concat([temp_df2, temp_df1], axis=1) #combine into a temp DataFrame\n",
        "              df = pd.concat([df, temp_df3], axis=0)\n",
        "              print(f'Finished scraping data for the {y} {s}.') #print a message for successful scraping for the current year and season type\n",
        "          else:\n",
        "              print(f'No data found for the {y} {s}.')\n",
        "      except requests.exceptions.RequestException as e: # Catch  errors related to the API request\n",
        "          print(f\"Error fetching data for {y} {s}: {e}\") #error message\n",
        "      except KeyError as e: #KeyError if expected keys are not in the response\n",
        "          print(f\"KeyError for {y} {s}: {e}. Response keys: {r.keys()}\") #KeyError message\n",
        "      except Exception as e: #Catch errors\n",
        "          print(f\"An unexpected error occurred for {y} {s}: {e}\") #error message\n",
        "\n",
        "      lag = np.random.uniform(low=5,high=40) #delay time\n",
        "      print(f'...waiting {round(lag,1)} seconds') #waiting time\n",
        "      time.sleep(lag)\n",
        "print(f'Process completed! Total run time; {round((time.time()-begin_loop)/60,2)}') #print the total time taken for the entire process\n",
        "df.to_excel('nba_player_data.xlsx', index=False) #save the collected data to an Excel file"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
